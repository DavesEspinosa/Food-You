

<div class="header" id="myHeader">
  <h1>{{currentUserInfo.name}} Recipes</h1>
</div>

<section class="content">
  <div class="add">
  <p><a href="/add-recipe"><i class="far fa-plus-square"></i> Add your recipes</a></p>
  </div>
  <div class="list-recipes-container">
    {{#each recipes}}
      {{> cardRecipe this}}
    {{/each}}
  </div>
</section>


{{#if errorMessage}}
  <p class="error-message">
    Ops! Seems that you haven't own recipes.
  </p>
{{/if}}

